    "name": "winex01/backpack-permission-manager",
    "description": "Group roles and permission field view of checklist in create of laravel backpack permission manager add-on",
    "license": "MIT",
    "authors": [
        {
            "name": "Winnie A. Damayo",
            "email": "winnie131212592@gmail.com",
            "homepage": "https://github.com/winex01"
        }
    ],
    "homepage": "https://github.com/winex01/backpack-permission-manager",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "BackpackPermissionManager"
    ],
    "require": {
        "illuminate/support": "~8|~9",
        "backpack/crud": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Winex01\\BackpackPermissionManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Winex01\\BackpackPermissionManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winex01\\BackpackPermissionManager\\AddonServiceProvider"
            ]
        }
    }
